    width: 400px;
    padding: 1px 0px;
    border: 1px solid #FFF;
    outline: none;
    font-size: 13px;
    color: #999;
}

#btn1 {
    margin: 0 auto;
    padding: 0px 0px 0px 15px;
    border: none;
    background: transparent none;
}

#taoba {
    margin: 0 auto;
    padding: 0;
    width: 960px;
    height: 18px;
    font: 12px/1.5 Tahoma,'Microsoft Yahei','Simsun';
    color: #444;
    text-align: center;
}

#foot {
    margin: 0 auto;
    padding: 10px 0 50px;
    width: 960px;
    height: 104px;
    border-top: 1px solid #CDCDCD;
    line-height: 1.8;
    color: #666;
    font: 12px/1.5 Tahoma,'Microsoft Yahei','Simsun';
}

    #foot a {
        height: 24px;
        color: #444;
    }

        #foot a:link, #topDiv a:visited {
            text-decoration: none;
        }

        #foot a:hover, #topDiv a:active {
            text-decoration: underline;
        }

#ftRight {
    width: 425px;
    height: 43px;
