    color:#333;
    font-weight:400;
    font-family:'Tahoma','Microsoft Yahei','Simsun';
}
.Stick{
    width:18px;
    height:18px;
    float:left;
    margin-left:3px;
    margin-top:10px;
}
.sName{
    width:638px;
    height:39px;
    float:left;
    font-size:14px;
    margin-top:3px;
    margin-left:8px;
}
.wTime{
    width:105px;
    height:39px;
    float:left;
    font-size:12px;
}
.wTime .One{
    width:105px;
    height:14px;
    display:block;
}
.wTime span{
    width:105px;
    height:15px;
    color:#999;
}
.sCount{
    width:60px;
    height:39px;
    float:left;
    font-size:12px;
}
.sCount .One{
    width:60px;
    height:14px;
    display:block;
}
.sCount span{
    width:60px;
    height:15px;
    color:#999;
}
.sLastName{
    width:105px;
    height:39px;
    float:left;
    font-size:12px;
}
.sLastName .One{
    width:105px;
    height:14px;
    display:block;
}
.sLastName span{
    width:105px;
    height:15px;
    color:#999;
}
.pnSeparation{
    width:938px;
    height:25px;
    background-color:#F2F2F2;
    border-bottom:1px solid #CDCDCD;
    margin:0 auto;
}
.Normal{
    margin-top:6px;
}


