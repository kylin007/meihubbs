.Writer{
    float:left;
    margin-right:80px;
}
.Reply_View{
    float:left;
    margin-right:10px;
}
.LastPost{
    float:left;
    margin-right:70px;
}
.Stickied, .Normal{
    width:958px;
    /*height:auto;*/
    overflow:hidden;
    zoom:1;
    margin:0px;
    padding:0px;
}
.Stickied ul, .Normal ul{
    margin:0px;
    padding:0px;
}
.Stickied li, .Normal li{
    width:938px;
    height:39px;
    list-style-type:none;
    line-height:2.5;
    float:left;
    margin-left:10px;
    border-bottom:1px solid #CDCDCD;
}
