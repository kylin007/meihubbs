     height:30px;
     position:absolute;
}
#S-top table .S-topimg1 {
        width: 16px;
        height: 14px;
        position:relative;
      background-image: url(../image/S_index.png);
      background-position:-7px -2px;
    }
#S-top table .S-topimg2 {
         width: 5px;
        height: 16px;
        position:relative;
      background-image: url(../image/S_index.png);
      background-position:-32px 0px;
}
#S-top table .S-topimg3 {
         width:5px;
        height: 16px;
        position:relative;
      background-image: url(../image/S_index.png);
      background-position:-32px 0px;
}

 #S-content  #S-center {
